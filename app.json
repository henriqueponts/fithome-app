{
  "expo": {
    "name": "FitHome",
    "slug": "fithome",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "fithome",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fithome.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#5B67F5",
        "foregroundImage": "./assets/images/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.fithome.app"
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#1A1B1E"
          }
        }
      ],
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#5B67F5",
          "sounds": ["default"],
          "androidMode": "default",
          "androidCollapsedTitle": "#{unread_notifications} notificações"
        }
      ]
    ],
    "experiments": {
      "reactCompiler": true
    }
  }
}
